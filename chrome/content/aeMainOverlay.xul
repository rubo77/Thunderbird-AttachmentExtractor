	</menu>
	<menuitem id="menu_ae_extractPatternDefault_context" 
			label="&attachmentextractor.extractpattern.label;"
			accesskey="&attachmentextractor.extractpattern.accesskey;" 
			ae_mru_menuitem="NONMRU"
