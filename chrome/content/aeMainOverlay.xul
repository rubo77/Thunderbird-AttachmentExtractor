	<menuitem id="menu_ae_extractToFolder_context" 
		label="&attachmentextractor.context.extracttofolder.label;"
		accesskey="&attachmentextractor.context.extracttofolder.accesskey;" 
		ae_mru_menuitem="NONMRU"
